{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=00000000-0000-0000-0000-000000000000;"
  },
  "Database": {
    "UseInMemoryDatabase": true
  },
  "EnableSwagger": true,
  "Authentication": {
    "PasswordSalt": "salt",
    "ForgotPasswordRequestExpirationMinutes": "10",
    "Cookie": {
      "ExpirationDays": "1",
      "Name": "__Host-SessionID",
      "Domain": "localhost"
    }
  }
}